<view><view class="tag-box"><view class="tab-list"><scroll-view class="uni-swiper-tab" id="tab-bar" scroll-x="{{true}}"><block wx:for="{{tagList}}" wx:for-item="tab" wx:for-index="index" wx:key="id"><view class="{{['swiper-tab-list',tabIndex==index?'active':'']}}" id="{{tab.id}}" data-current="{{index}}" data-event-opts="{{[['tap',[['tabChange',[index]]]]]}}" bindtap="__e">{{tab.name}}</view></block></scroll-view></view><view data-event-opts="{{[['tap',[['switchTags',['$event']]]]]}}" class="more" bindtap="__e"><block wx:if="{{!tagStatus}}"><text class="iconfont"></text></block><block wx:else><text class="iconfont"></text></block></view></view><block wx:if="{{tagStatus}}"><view data-event-opts="{{[['touchmove',[['stopPrevent',['$event']]]]]}}" class="cate-box" catchtouchmove="__e"><view class="brand"><view class="brand-th flex-row"><view class="flex-left">公司/品牌</view><navigator class="flex-right" url="/pages/user_center/user_setting/user_setting">修改</navigator></view><block wx:if="{{user.brand}}"><view class="brand-td flex-row"><view class="flex-left"><pack-tag vue-id="c81cd1ea-1" selected="{{true}}" text="{{user.brand.name}}" bind:__l="__l"></pack-tag></view></view></block></view><view class="gap"></view><view class="cate"><text>关注的类别</text><view class="cate-list"><block wx:for="{{category}}" wx:for-item="item" wx:for-index="cIndex" wx:key="cIndex"><view data-event-opts="{{[['tap',[['selectCate',[cIndex]]]]]}}" class="cate-item" bindtap="__e"><pack-tag vue-id="{{'c81cd1ea-2-'+cIndex}}" selected="{{item.selected}}" text="{{item.name}}" bind:__l="__l"></pack-tag></view></block></view></view><view class="gap"></view><view data-event-opts="{{[['tap',[['saveCate',['$event']]]]]}}" class="cate-sure" bindtap="__e"><view class="cate-sure-btn">确定</view></view></view></block><view class="article-list"><view class="gap"></view><view class="uni-list"><block wx:for="{{articleList}}" wx:for-item="article" wx:for-index="key" wx:key="key"><view class="uni-list-cell" hover-class=